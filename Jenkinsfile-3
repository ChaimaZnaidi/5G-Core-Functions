pipeline {
   agent any
  stages {
    stage('run collection') {
      steps {
        sh 'docker --privileged --version'
        sh 'docker run --privileged -t postman/newman run -h'
        sh 'docker run --privileged	-v ${WORKSPACE}:/etc/newman --workdir /etc/newman -t postman/newman run NRF_NF_REGISTER.json --color off --disable-unicode'
      }
    }
  }
}
